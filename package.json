{
	"name": "vigilare",
	"version": "0.7.0",
	"type": "module",
	"license": "MIT",
	"scripts": {
		"build": "vite build && tsc --emitDeclarationOnly",
		"dev": "vite",
		"lint:lint-staged": "lint-staged",
		"lint:eslint": "eslint --max-warnings 0 \"src/**/*.{js,jsx,ts,tsx}\" --fix",
		"lint:prettier": "prettier --write --log-level warn \"src/**/*.{js,ts,json,jsx,tsx,css,less,scss,vue,html,md}\"",
		"lint:prettierall": "prettier --write --log-level warn \".\"",
		"lint:stylelint": "stylelint --cache --fix \"**/*.{less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
		"lint": "pnpm lint:eslint && pnpm lint:prettier && pnpm lint:stylelint",
		"preview": "vite preview",
		"prepare": "husky"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"eslint --fix",
			"prettier --write"
		],
		"{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
			"prettier --write--parser json"
		],
		"package.json": [
			"prettier --write"
		],
		"*.{scss,less,styl}": [
			"stylelint --fix",
			"prettier --write"
		],
		"*.md": [
			"prettier --write"
		]
	},
	"engines": {
		"node": ">=18.12.0",
		"pnpm": ">=9.0.2"
	},
	"dependencies": {
		"@icon-park/react": "^1.4.2",
		"antd": "^5.24.6",
		"dayjs": "^1.11.13",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-transition-group": "^4.4.5",
		"recharts": "^2.15.2",
		"zustand": "^5.0.3"
	},
	"devDependencies": {
		"@babel/eslint-parser": "^7.27.0",
		"@babel/plugin-proposal-decorators": "^7.25.9",
		"@commitlint/cli": "^19.5.0",
		"@commitlint/config-conventional": "^19.5.0",
		"@iconify/react": "^5.0.2",
		"@types/lodash-es": "^4.17.12",
		"@types/node": "^20.17.6",
		"@types/react": "^18.3.20",
		"@types/react-dom": "^18.3.6",
		"@types/react-transition-group": "^4.4.12",
		"@types/recharts": "^2.0.1",
		"@typescript-eslint/eslint-plugin": "^7.18.0",
		"@typescript-eslint/parser": "^7.18.0",
		"@unocss/preset-rem-to-px": "^0.61.9",
		"@vitejs/plugin-react": "^4.3.4",
		"autoprefixer": "^10.4.20",
		"eslint": "^8.57.1",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-babel": "^5.3.1",
		"eslint-plugin-prettier": "^5.2.1",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^4.6.2",
		"eslint-plugin-react-refresh": "^0.4.19",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"husky": "^9.1.6",
		"less": "^4.3.0",
		"lint-staged": "^15.2.10",
		"lodash-es": "^4.17.21",
		"prettier": "^3.3.3",
		"stylelint": "^16.10.0",
		"stylelint-config-recess-order": "^5.1.1",
		"stylelint-config-standard": "^36.0.1",
		"stylelint-less": "^3.0.1",
		"stylelint-prettier": "^5.0.2",
		"terser": "^5.39.0",
		"typescript": "^5.8.3",
		"unocss": "^0.61.9",
		"vite": "^4.5.12",
		"vite-plugin-compression": "^0.5.1",
		"vite-plugin-pwa": "^0.16.7"
	}
}
